    "nodemon": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-export-table-to-excel": "^1.0.6",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.11.2",
    "react-table": "^7.8.0",
